
@{
    Layout = null;
}
@model List<LWT.Model.ManageLimit>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetManageLimit</title>
</head>
<body>
    <div style="text-align:center;">
        <input type="type" name="BuyerName" value="" placeholder="请输入企业名称" /><button style="background-color:#4cff00;color:white">查询</button>
        <span>审核状态<input type="type" name="State" value="" placeholder="选择审核状态" /></span>
        <table class="table table-border">
            <tr>
                <th>序号</th>
                <th>企业名称</th>
                <th>结算方式</th>
                <th>总额度</th>
                <th>申请额度</th>
                <th>运营商审核装状态</th>
                <th>审批额度</th>
            </tr>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.BuyerName</td>
                <td>@item.SettLemeny</td>
                <td>@item.TotalAmount</td>
                <td>@item.ApplyAmount </td>
                <td>@item.State</td>
                <td>@item.AppRoveLimit </td>
                <td><a href="/Settle/ThinMaLimite/">查看</a> </td>
                <td style="text-align:center;"><span class="label label-info "><a href="#" onclick="show('聚聚', '/Settle/ThinMaLimite/', 800, 600)">查看</a></span></td>
            </tr>
            }
        </table>
    </div>
</body>
</html>
<script type="text/javascript" src="/lib/laypage/1.2/laypage.js"></script>
<script type="text/javascript">
    function show(title, url, w, d) {
        layer_show(title, url, w, d)
    }
</script>
