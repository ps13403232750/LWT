
@{
    Layout = null;
}
@model List<LWT.Model.PurChaseSettle>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetPurChaseSettle</title>
</head>
<body>
    <h1>采购结算列表</h1>
    <table class="table table-border">
        <tr>
            <th>序号</th>
            <th>采购商名称</th>
            <th>结算类型</th>
            <th>结算周期</th>
            <th>状态</th>
            <th>操作</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.BuyerName</td>
            <td>@item.SettleMeny</td>
            <td>@item.SettleDays</td>
            <td>@item.SettleState</td>
            <td><a href="/Settle/GetManageLimit/">查看</a></td>
        </tr>
        }
    </table>
</body>
</html>
